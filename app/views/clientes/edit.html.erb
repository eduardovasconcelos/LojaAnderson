<h1>Editando Cliente</h1>
<div id="tabelas">
    <% form_for(@cliente) do |f| %>
      <%= f.error_messages %>

      <p>
        <%= f.label :nome %><br />
        <%= f.text_field :nome %>
      </p>
      <p>
        <%= f.label :endereco %><br />
        <%= f.text_field :endereco %>
      </p>
      <p>
        <%= f.label :cpf %><br />
        <%= f.text_field :cpf %>
      </p>
      <p>
        <%= f.label :rg %><br />
        <%= f.text_field :rg %>
      </p>
      <p>
        <%= f.label :telefone %><br />
        <%= f.text_field :telefone %>
      </p>
      <p>
         <%= f.label :EMAIL %><br />
         <%= f.text_field :email %>
      </p>
      <p>
        <%= f.submit "Atualizar" %>
      </p>
    <% end %>
    <%= link_to 'Voltar', clientes_path %><br><br>
</div>
